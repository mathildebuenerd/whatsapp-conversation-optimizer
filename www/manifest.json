{
  "manifest_version": 2,
  "name": "Conversaton Optimizer",
  "short_name": "conversation-optimizr",
  "version": "0.1.0",
  "content_security_policy": "script-src 'self' https://www.gstatic.com/ https://*.firebaseio.com https://www.googleapis.com; 'sha256-N0k0/YcUD0DG7+FEelEAXKBs7MLL5QYx8+LoBNMtJJY='; object-src 'self'",
  "sidebar_action": {
    "default_title": "Conversation Optimizer",
    "default_panel": "index.html",
    "default_icon": "sidebar_icon.png"
  },
  "permissions": [
    "storage",
    "tabs",
    "*://*/*",
    "*://googleapis.com/*"
  ]
}
